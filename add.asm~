		.ORIG 0x3000;
		AND R1, R1, #0;
		AND R3, R3, #0;
		AND R4, R4, #0;   Clearing registers
		ADD R3, R3, #-10; Initialize first -10
		ADD R4, R4, #-1;	Initialize to -1 to double the -10
		LEA R5 address;
		LDW R2, R5, #0;
loop 		ADD R1, R1, R2;
		ADD R5, R5, #2;
		LDW R2, R5, #0;
		ADD R3, R3, #1;
		BRn	loop;
		ADD R3, R3, #-10;
		ADD R4, R4, #1;
		BRnz loop;
		LEA R5, store;
		STW R1, R5, #0;
		HALT;
address 	.FILL  0xC000;
store		.FILL  0xC014;
		.END;
		
	
	
	


	
	
	
	
